<?xml version="1.0" encoding="utf-8"?>
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android"
               android:transitionOrdering="together">
    <fade
        android:duration="@integer/animation_fade_out"
        android:fadingMode="fade_out"/>
    <changeBounds
        android:duration="@integer/animation_change_bounds_collapse"
        android:interpolator="@android:interpolator/anticipate_overshoot"
        android:startDelay="@integer/animation_change_bounds_collapse_start_delay"/>
    <fade
        android:duration="@integer/animation_fade_in"
        android:fadingMode="fade_in"
        android:startDelay="@integer/animation_fade_in_start_delay"/>
</transitionSet>
